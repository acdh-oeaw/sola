{
  "name": "sola",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "build:mock": "cross-env NEXT_PUBLIC_API_MOCKING=enabled yarn build",
    "cms:dev": "netlify-cms-proxy-server",
    "create:favicons": "ts-node -r tsconfig-paths/register -P scripts/tsconfig.json scripts/favicons",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "dev:mock": "cross-env NEXT_PUBLIC_API_MOCKING=enabled yarn dev",
    "export": "next export",
    "format": "prettier . -l --ignore-path .gitignore",
    "format:fix": "yarn format --write",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "msw:update": "npx msw init public",
    "prebuild": "rimraf .next && yarn create:favicons",
    "preexport": "rimraf out",
    "start": "next start",
    "storybook:build": "build-storybook -s public,src/styles -c stories",
    "storybook:dev": "start-storybook -p 6006 -s public,src/styles -c stories --ci",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@juggle/resize-observer": "^3.2.0",
    "@next/bundle-analyzer": "^10.0.5",
    "@popperjs/core": "^2.6.0",
    "@react-aria/button": "^3.3.0",
    "@react-aria/focus": "^3.2.3",
    "@react-aria/i18n": "^3.2.0",
    "@react-aria/interactions": "^3.3.2",
    "@react-aria/listbox": "^3.2.3",
    "@react-aria/overlays": "^3.6.0",
    "@react-aria/progress": "^3.1.1",
    "@react-aria/separator": "^3.1.1",
    "@react-aria/ssr": "^3.0.1",
    "@react-aria/tabs": "^3.0.0-alpha.2",
    "@react-aria/textfield": "^3.2.2",
    "@react-aria/utils": "^3.5.0",
    "@react-stately/collections": "^3.3.0",
    "@react-stately/list": "^3.2.2",
    "@react-stately/overlays": "^3.1.1",
    "@react-stately/tabs": "^3.0.0-alpha.0",
    "@stefanprobst/favicons": "^1.0.6",
    "@stefanprobst/next-error-boundary": "^1.0.7",
    "@stefanprobst/next-mdx": "^1.0.0",
    "@stefanprobst/next-page-metadata": "^1.0.8",
    "@stefanprobst/remark-excerpt": "^1.0.2",
    "@stefanprobst/remark-extract-yaml-frontmatter": "^1.0.1",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.0",
    "clsx": "^1.1.1",
    "d3-array": "^2.9.1",
    "d3-axis": "^2.0.0",
    "d3-force": "^2.1.1",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "focus-visible": "^5.2.0",
    "next": "^10.0.5",
    "postcss": "^8.2.2",
    "postcss-focus-visible": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-popper": "^2.2.4",
    "react-query": "^3.5.16",
    "rehype-stringify": "^8.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-parse": "^9.0.0",
    "remark-rehype": "^8.0.0",
    "remark-stringify": "^9.0.1",
    "strip-markdown": "^4.0.0",
    "tailwindcss": "^2.0.2",
    "unified": "^9.2.0",
    "use-composed-ref": "^1.1.0",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@next/eslint-plugin-next": "^10.0.5",
    "@next/plugin-storybook": "^10.0.5",
    "@stefanprobst/commitlint-config": "^1.0.1",
    "@stefanprobst/eslint-config-next": "^1.0.9",
    "@stefanprobst/eslint-config-next-component": "^1.0.9",
    "@stefanprobst/prettier-config": "^1.0.1",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.6.0",
    "@types/d3-array": "^2.8.0",
    "@types/d3-axis": "^2.0.0",
    "@types/d3-force": "^2.1.0",
    "@types/d3-scale": "^3.2.2",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-zoom": "^2.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "husky": "^4.3.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "msw": "^0.25.0",
    "netlify-cms-proxy-server": "^1.3.8",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.11.7",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=12.9.0",
    "yarn": "^1.22.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ]
  },
  "commitlint": {
    "extends": [
      "@stefanprobst/commitlint-config"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@stefanprobst/eslint-config-next",
      "plugin:jest-dom/recommended",
      "plugin:testing-library/react"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "tsc && lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --cache --fix",
    "*.(css|json|md|mdx)": "prettier --write"
  },
  "prettier": "@stefanprobst/prettier-config"
}
